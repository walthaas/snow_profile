{
  "env": {
    "es2020": true,
    "node": true,
    "jest" : true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-tabs": 1,
    "eqeqeq": [2, "smart"],
    "guard-for-in": 2,
    "no-undef": 2,
    "no-unused-vars": 0,
    "strict": 2,
    "new-cap": 0,
    "quotes": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "no-new": 0,
    "no-alert": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "no-constant-condition": 0
  }
}
